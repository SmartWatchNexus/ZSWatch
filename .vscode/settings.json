{
    "nrf-connect.applications": [
        "${workspaceFolder}/app",
        "${workspaceFolder}\\app",
        "${workspaceFolder}/zephyr/samples/boards/nrf/system_off"
    ],
    "nrf-connect.boardRoots": [
        "${workspaceFolder}/app",
        "${workspaceFolder}\\app",
        "${workspaceFolder}/zephyr",
        "${workspaceFolder}\\zephyr",
    ],
    "nrf-connect.debugging.bindings": {
        "${workspaceFolder}/app/build": "Launch build",
        "${workspaceFolder}/app/build_dk": "Launch build_dk",
        "${workspaceFolder}/app/build_debug_v5_rtt": "Launch build_debug_v5_rtt",
        "${workspaceFolder}/app/build_dbg_v4": "Launch build_dbg_v4",
        "${workspaceFolder}/zephyr/samples/boards/nrf/system_off/build_v5": "Launch build_v5"
    },
    "editor.defaultFormatter": "chiehyu.vscode-astyle",
    "astyle.astylerc": "${workspaceRoot}/app/tools/astyle.cfg",
    "C_Cpp.configurationWarnings": "disabled",
    "terminal.integrated.env.linux": {
        "GTK_PATH": ""
    },
    "search.useIgnoreFiles": false,
    "files.associations": {
        "*.x": "c",
        "zsw_rtt_flash_loader.h": "c",
        "*.cx": "c"
    }
